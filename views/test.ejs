<table id="historique" style="display: none;">
    <thead>
        <tr>
            <th>Matricule</th>
            <th>Du</th>
            <th>Au</th>
            <th>Nature</th>
            <th>Nombre de jours</th>
        </tr>
    </thead>
    <tbody>
        <% leaveList.forEach(leave => { %>
            <tr>
                <td><a href="/personnel/<%= leave.empId %> "><%= leave.matricule %></a></td>
                <td><%= leave.startDate %> </td>
                <td><%= leave.endDate %> </td>
                <td><%= leave.type %> </td>
                <td><%= leave.numberOfDays %> jours </td>
            </tr>
        <% }) %> 
    </tbody>
</table>